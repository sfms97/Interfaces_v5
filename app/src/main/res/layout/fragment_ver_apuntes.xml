<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup238"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelime_100">

        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="412dp"
            android:layout_height="733dp"
            android:layout_marginTop="@dimen/_10pxv"
            android:clipToPadding="false"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup230"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageGroup37"
                    style="@style/groupStyleorange_500_e5"
                    android:layout_width="411dp"
                    android:layout_height="49dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group19"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/txtFsica"
                    style="@style/txtSansitaSwashedromanregular36"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="36dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    app:layout_constraintBottom_toTopOf="@+id/txtApuntes1fech"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/lbl_f_sica" />

                <TextView
                    android:id="@+id/txtApuntes1fech"
                    style="@style/txtSansitaSwashedromanregular18_1"
                    android:layout_width="327dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="36dp"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:singleLine="true"
                    app:layout_constraintBottom_toTopOf="@+id/imageImage1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.505"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="@string/msg_apuntes_1_fech" />

                <ImageView
                    android:id="@+id/imageImage1"
                    android:layout_width="318dp"
                    android:layout_height="353dp"
                    android:layout_marginBottom="48dp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/imageZoomIn"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/img_image1" />

                <ImageView
                    android:id="@+id/imageZoomIn"
                    android:layout_width="66dp"
                    android:layout_height="52dp"
                    android:layout_marginBottom="12dp"
                    android:clickable="true"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toTopOf="@+id/imageGroup37"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.91"
                    app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/img_zoomin" />

                <ImageView
                    android:id="@+id/imageView26"
                    android:layout_width="56dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:layout_constraintEnd_toStartOf="@+id/imageView27"
                    app:layout_constraintHorizontal_bias="0.321"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup37"
                    app:layout_constraintTop_toTopOf="@+id/imageView27"
                    app:srcCompat="@drawable/img_group_1" />

                <ImageView
                    android:id="@+id/imageView27"
                    android:layout_width="56dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup37"
                    app:layout_constraintTop_toTopOf="@+id/imageGroup37"
                    app:layout_constraintVertical_bias="0.555"
                    app:srcCompat="@drawable/img_group_12" />

                <ImageView
                    android:id="@+id/imageView28"
                    android:layout_width="65dp"
                    android:layout_height="32dp"
                    android:clickable="true"
                    app:layout_constraintEnd_toEndOf="@+id/imageGroup37"
                    app:layout_constraintHorizontal_bias="0.704"
                    app:layout_constraintStart_toEndOf="@+id/imageView27"
                    app:layout_constraintTop_toTopOf="@+id/imageView27"
                    app:srcCompat="@drawable/img_group_10" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
