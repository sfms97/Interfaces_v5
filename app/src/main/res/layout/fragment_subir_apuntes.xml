<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup347"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelime_100">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup340"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageGroup73"
                style="@style/groupStyleorange_500_e5"
                android:layout_width="411dp"
                android:layout_height="49dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_group19"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/txtSubirApuntes"
                style="@style/txtSansitaSwashedromanregular36"
                android:layout_width="304dp"
                android:layout_height="62dp"
                android:layout_marginTop="40dp"
                android:ellipsize="end"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/lbl_subir_apuntes"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtTtulo"
                style="@style/txtSansitaSwashedromanregular18_1"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="20dp"
                android:ellipsize="end"
                android:gravity="start"
                android:singleLine="true"
                android:text="@string/lbl_t_tulo"
                app:layout_constraintBottom_toTopOf="@+id/viewRectangle10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/txtSeleccionaLaA"
                style="@style/txtSansitaSwashedromanregular18_1"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="16dp"
                android:ellipsize="end"
                android:gravity="start"
                android:singleLine="true"
                android:text="@string/msg_selecciona_la_a"
                app:layout_constraintBottom_toTopOf="@+id/asignaturasSubirApuntes"
                app:layout_constraintEnd_toEndOf="parent"

                app:layout_constraintStart_toStartOf="parent" />

            <View
                android:id="@+id/viewRectangle10"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/rectangle_bg_gray_400_93_radius_20"
                app:layout_constraintBottom_toTopOf="@+id/txtSeleccionaLaA"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <View
                android:id="@+id/viewRectangle17"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/rectangle_bg_gray_400_93_radius_20"
                app:layout_constraintBottom_toTopOf="@+id/txtSeleccionaElF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/txtSeleccionaElF"
                style="@style/txtSansitaSwashedromanregular18_1"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="4dp"
                android:ellipsize="end"
                android:gravity="start"
                android:singleLine="true"
                android:text="@string/msg_selecciona_el_f"
                app:layout_constraintBottom_toTopOf="@+id/viewRectangle21"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/txtSubirApuntes1"
                style="@style/txtSolidRounded"
                android:layout_width="209dp"
                android:layout_height="48dp"
                android:layout_marginBottom="104dp"
                android:clickable="true"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingEnd="@dimen/_4pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_subir_apuntes2"
                app:layout_constraintBottom_toTopOf="@+id/imageGroup73"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <View
                android:id="@+id/viewRectangle21"
                android:layout_width="233dp"
                android:layout_height="39dp"
                android:layout_marginBottom="40dp"
                android:background="@drawable/rectangle_bg_gray_400_93_radius_20"
                app:layout_constraintBottom_toTopOf="@+id/txtSubirApuntes1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/imageGroup"
                android:layout_width="@dimen/_22pxh"
                android:layout_height="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_15"
                app:layout_constraintBottom_toTopOf="@+id/txtSubirApuntes1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.165"
                app:layout_constraintStart_toEndOf="@+id/viewRectangle21"
                app:layout_constraintTop_toBottomOf="@+id/txtSeleccionaElF"
                app:layout_constraintVertical_bias="0.046" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:clickable="true"
                app:layout_constraintEnd_toStartOf="@+id/imageView20"
                app:layout_constraintHorizontal_bias="0.38"
                app:layout_constraintStart_toStartOf="@+id/imageGroup73"
                app:layout_constraintTop_toTopOf="@+id/imageGroup73"
                app:srcCompat="@drawable/img_group_1"
                tools:clickable="true" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageGroup73"
                app:layout_constraintStart_toStartOf="@+id/imageGroup73"
                app:layout_constraintTop_toTopOf="@+id/imageGroup73"
                app:srcCompat="@drawable/img_group_12" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:clickable="true"
                app:layout_constraintEnd_toEndOf="@+id/imageGroup73"
                app:layout_constraintHorizontal_bias="0.682"
                app:layout_constraintStart_toEndOf="@+id/imageView20"
                app:layout_constraintTop_toTopOf="@+id/imageGroup73"
                app:srcCompat="@drawable/img_group_10" />

            <EditText
                android:id="@+id/tituloSubirApuntes"
                android:layout_width="216dp"
                android:layout_height="39dp"
                android:layout_weight="1"
                android:ems="10"
                android:hint="Titulo"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/viewRectangle10"
                app:layout_constraintEnd_toEndOf="@+id/viewRectangle10"
                app:layout_constraintStart_toStartOf="@+id/viewRectangle10"
                app:layout_constraintTop_toTopOf="@+id/viewRectangle10"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/asignaturasSubirApuntes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="Asignatura"
                android:inputType="textPersonName"
                app:layout_constraintBottom_toBottomOf="@+id/viewRectangle17"
                app:layout_constraintEnd_toEndOf="@+id/viewRectangle17"
                app:layout_constraintHorizontal_bias="0.6"
                app:layout_constraintStart_toStartOf="@+id/viewRectangle17"
                app:layout_constraintTop_toTopOf="@+id/viewRectangle17"
                app:layout_constraintVertical_bias="0.615" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
