<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup260"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelime_100">

        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="412dp"
            android:layout_height="744dp"
            android:layout_marginTop="@dimen/_10pxv"
            android:clipToPadding="false"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup257"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/imageGroup52"
                    android:layout_width="407dp"
                    android:layout_height="59dp"
                    android:layout_marginBottom="16dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group19"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/txtAsignaturas"
                    style="@style/txtSansitaSwashedromanregular36"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="88dp"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="Asignaturas"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="@string/lbl_asignaturas" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintGroup251"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toTopOf="@+id/imageGroup52"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.505"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/constraintGroup256"
                    app:layout_constraintVertical_bias="0.617">

                    <View
                        android:id="@+id/viewRectangle19"
                        android:layout_width="@dimen/_97pxh"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:background="@drawable/rectangle_bg_amber_700_radius_20"
                        android:clickable="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/viewRectangle20"
                        app:layout_constraintHorizontal_bias="0.08"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/viewRectangle20"
                        android:layout_width="@dimen/_97pxh"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:background="@drawable/rectangle_bg_amber_700_radius_20"
                        android:clickable="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.37"
                        app:layout_constraintStart_toEndOf="@+id/viewRectangle19"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txtEliminar"
                        style="@style/txtSansitaSwashedromanregular18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="Eliminar"
                        app:layout_constraintBottom_toBottomOf="@+id/viewRectangle20"
                        app:layout_constraintEnd_toEndOf="@+id/viewRectangle20"
                        app:layout_constraintStart_toStartOf="@+id/viewRectangle20"
                        app:layout_constraintTop_toTopOf="@+id/viewRectangle20"
                        tools:text="@string/lbl_eliminar" />

                    <TextView
                        android:id="@+id/txtAadir"
                        style="@style/txtSansitaSwashedromanregular18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="AÃ±adir"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/viewRectangle19"
                        app:layout_constraintStart_toStartOf="@+id/viewRectangle19"
                        app:layout_constraintTop_toTopOf="@+id/viewRectangle19"
                        tools:text="@string/lbl_a_adir" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintGroup256"
                    style="@style/groupStylegray_400_8ccornerRadius"
                    android:layout_width="322dp"
                    android:layout_height="305dp"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtAsignaturas">

                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtFsica"
                                style="@style/txtSolidRounded_3"
                                android:layout_width="152dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_137pxh"
                                android:clickable="true"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingBottom="@dimen/_11pxv"
                                android:singleLine="true"
                                android:text="Fisica"
                                tools:text="@string/lbl_f_sica" />

                            <TextView
                                android:id="@+id/txtProgramacin"
                                style="@style/txtSolidRounded_3"
                                android:layout_width="184dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="28dp"
                                android:layout_marginEnd="@dimen/_84pxh"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingEnd="@dimen/_8pxh"
                                android:paddingBottom="@dimen/_11pxv"
                                android:singleLine="true"
                                android:text="Programacion"
                                tools:text="@string/lbl_programaci_n" />

                            <TextView
                                android:id="@+id/txtHistoria"
                                style="@style/txtSolidRounded_3"
                                android:layout_width="180dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_137pxh"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingBottom="@dimen/_11pxv"
                                android:singleLine="true"
                                android:text="Historia"
                                tools:text="@string/lbl_historia" />

                            <TextView
                                android:id="@+id/txtInterfacesDeU"
                                style="@style/txtSolidRounded_3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_47pxh"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingEnd="@dimen/_3pxh"
                                android:paddingBottom="@dimen/_11pxv"
                                android:singleLine="true"
                                android:text="Interfaces de ususario"
                                tools:text="@string/msg_interfaces_de_u" />

                        </LinearLayout>
                    </ScrollView>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    app:layout_constraintEnd_toStartOf="@+id/imageView2"
                    app:layout_constraintHorizontal_bias="0.41"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/imageView2"
                    app:srcCompat="@drawable/img_group_1" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    app:layout_constraintBottom_toBottomOf="@+id/imageGroup52"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.509"
                    app:layout_constraintStart_toStartOf="@+id/imageGroup52"
                    app:layout_constraintTop_toTopOf="@+id/imageGroup52"
                    app:srcCompat="@drawable/img_group_12" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    app:layout_constraintEnd_toEndOf="@+id/imageGroup52"
                    app:layout_constraintHorizontal_bias="0.68"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView2"
                    app:srcCompat="@drawable/img_group_10" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
