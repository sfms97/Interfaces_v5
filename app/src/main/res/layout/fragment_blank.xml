                    android:layout_height="45dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:hint="Titulo"
                    app:layout_constraintBottom_toTopOf="@+id/txtAsignatura"
                    app:layout_constraintEnd_toEndOf="@+id/tituloEventos"
                    app:layout_constraintHorizontal_bias="0.329"
                    app:layout_constraintStart_toStartOf="@+id/tituloEventos"
                    app:layout_constraintTop_toBottomOf="@+id/txtTtulo"
                    app:layout_constraintVertical_bias="1.0" />

                <EditText
                    android:id="@+id/asignaturaEventos"
                    android:layout_width="247dp"
                    android:layout_height="46dp"
                    android:ems="10"
                    android:hint="Asignatura"
                    android:inputType="textPersonName"
                    app:layout_constraintBottom_toBottomOf="@+id/textInputEditText2"
                    app:layout_constraintEnd_toEndOf="@+id/textInputEditText2"
                    app:layout_constraintHorizontal_bias="0.155"
                    app:layout_constraintStart_toStartOf="@+id/textInputEditText2"
                    app:layout_constraintTop_toTopOf="@+id/textInputEditText2"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/comentariosEventos"
                    android:layout_width="281dp"
                    android:layout_height="64dp"
                    android:ems="10"
                    android:hint="Comentarios"
