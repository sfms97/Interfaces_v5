                    app:layout_constraintBottom_toBottomOf="@+id/textInputEditText"
                    app:layout_constraintEnd_toEndOf="@+id/textInputEditText"
                    app:layout_constraintHorizontal_bias="0.562"
                    app:layout_constraintStart_toStartOf="@+id/textInputEditText"
                    app:layout_constraintTop_toTopOf="@+id/textInputEditText"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/editTextDate3"
                    android:layout_width="105dp"
                    android:layout_height="44dp"
                    android:ems="10"
                    android:hint="20/01/1900"
                    android:inputType="date"
                    app:layout_constraintBottom_toBottomOf="@+id/txt"
                    app:layout_constraintEnd_toEndOf="@+id/txt"
                    app:layout_constraintHorizontal_bias="0.352"
                    app:layout_constraintStart_toStartOf="@+id/txt"
                    app:layout_constraintTop_toTopOf="@+id/txt"
